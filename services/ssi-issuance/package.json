    "@nestjs/core": "^9.0.7",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/platform-fastify": "^9.0.7",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@opentelemetry/exporter-jaeger": "^1.5.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.31.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.31.0",
    "@opentelemetry/instrumentation-fastify": "^0.28.0",
    "@opentelemetry/instrumentation-http": "^0.31.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.30.0",
    "@opentelemetry/sdk-trace-node": "^1.5.0",
    "@ungap/structured-clone": "^1.0.1",
    "backoff-rxjs": "^7.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "convict": "^6.2.3",
    "dayjs": "^1.11.4",
    "fastify-swagger": "^5.2.0",
    "jsonld": "^6.0.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.1.1",
    "pino-http": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.7",
    "@types/convict": "^6.1.1",
    "@types/jest": "^28.1.6",
    "@types/jest-when": "^3.5.2",
    "@types/jsonld": "^1.5.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.2",
    "@types/semver": "^7.3.10",
    "@types/ungap__structured-clone": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-when": "^3.5.1",
    "lint-staged": "^13.0.3",
    "openapi-typescript": "^5.4.1",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-essentials": "^9.2.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
