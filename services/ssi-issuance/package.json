  "homepage": "https://gitlab.com/gaia-x/data-infrastructure-federation-services/not/notarization-service",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:gaia-x/data-infrastructure-federation-services/not/notarization-service.git"
