    "node": ">=16.13.1",
    "npm": ">=8.1.2"
  },
  "packageManager": "npm@10.1.0",
  "scripts": {
    "clean": "rimraf dist coverage *.tsbuildinfo",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:watch": "nest build --watch",
    "build:production": "nest build -p tsconfig.production.json",
    "start": "nest start --preserveWatchOutput | pino-pretty -c -t -l -S",
    "start:debug": "nest start --debug 0.0.0.0:9229 --preserveWatchOutput | pino-pretty -c -t -l -S",
    "start:dev": "nest start --watch --preserveWatchOutput | pino-pretty -c -t -l -S",
    "start:dev:debug": "nest start --debug 0.0.0.0:9229 --watch | pino-pretty -c -t -l -S",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --testTimeout==999999 --bail",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --runInBand --config ./test/jest.config.js",
    "test:e2e:cov": "jest --runInBand --config ./test/jest.config.js --coverage",
    "test:e2e:debug": "node --inspect node_modules/.bin/jest --runInBand --testTimeout==999999 --bail --config ./test/jest.config.js ",
    "lint": "eslint --fix",
    "lint:all": "npm run lint -- .",
    "format": "prettier --write",
    "format:all": "npm run format -- .",
    "prepare": "cd ../.. && husky install ./services/ssi-issuance/.husky",
    "generate-acapy-types": "node ./scripts/generate-acapy-types.mjs"
  },
  "dependencies": {
    "@fastify/helmet": "^9.1.0",
